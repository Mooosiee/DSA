class Solution {
public:
    int makeTheIntegerZero(int num1, int num2) {
        for(int k = 0;k <= 60;k++){
            long long val = num1 - (1LL * k * num2);
            if(val < 0) return -1;
            if( __builtin_popcountll(val) <= k && k <= val) return k;
        }
        return -1;
    }
};
